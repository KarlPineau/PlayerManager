{# src/DnDRules/WeaponBundle/Resources/views/WeaponType/layoutAdmin.html.twig #}
 
{% extends "::admin.html.twig" %}

{% block title %}
    Types d'Arme - {{ parent() }}
{% endblock %}

{% block follow %}
    <meta name="robots" content="noindex,nofollow" />
{% endblock %}
    
{% block breadcrumb %}
    {{ parent() }}
    <li><a href="{{ path('dndrules_weapon_weapon_home') }}">Armes</a></li>
    {% if app.request.get('_route') == 'dndrules_weapon_weapontype_home' %}
        <li class="active">Types</li>
    {% else %}
        <li><a href="{{ path('dndrules_weapon_weapontype_home') }}">Types</a></li>
    {% endif %}
{% endblock %}

{% block body %}
    {% block PM_body %}
    {% endblock %}
{% endblock %}
